@model Products

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Supply</h2>

@if (!string.IsNullOrEmpty(ViewBag.AlertMessage))
{
    <script>alert("@ViewBag.AlertMessage");</script>
}
<div class="row">
    <div class="col-md-4">
        <form asp-action="SupplyAction">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="SupplierKey" class="control-label"></label>
                <input asp-for="SupplierKey" class="form-control" />
                <span asp-validation-for="SupplierKey" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <input asp-for="Id" class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TransactionQuantity" class="control-label"></label>
                <input asp-for="TransactionQuantity" class="form-control" />
                <span asp-validation-for="TransactionQuantity" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Supply" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>